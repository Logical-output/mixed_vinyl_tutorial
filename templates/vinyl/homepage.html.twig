{% extends "base.html.twig" %}

{% block title %} Create a new record | {{ parent() }} {% endblock title %}

{% block body %}

    <h1>{{ title }}</h1>

    <ul>        
        {% for track in tracks %}            
            <li>{{ track.song }} - {{ track.artist }}</li>
        {% endfor %}        
    </ul>
    
{% endblock body %}
